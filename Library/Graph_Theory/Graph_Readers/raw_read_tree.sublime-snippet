	<content><![CDATA[
int n;
cin >> n;
vector<vector<int>> adj(n);
for(auto i = 0; i < n - 1; ++ i){
	int u, v;
	cin >> u >> v, -- u, -- v;
	adj[u].push_back(v), adj[v].push_back(u);
}
]]></content>
